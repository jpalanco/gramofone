config system 'system'
	option hostname	fonera
	option timezone	UTC
	option log_enabled false
	option log_url ''

config button
	option button 'wps'
	option action 'pressed'
	option handler '/bin/ubus call buttond pressed'

config button
	option button 'wps'
	option action 'released'
	option handler '/bin/ubus call buttond released'
